drop database scit;
create database scit;

use scit;

drop table if exists phonebook;

create table sample
(
	id int auto_increment primary key,
    name varchar(30) not null,
    age int
);

insert into sample
(name, age)
values
('홍길동', 45);

commit;

select * from sample;

-- 2024년 12월 2일 전화번호부 프로젝트
create table phonebook
(
	id int auto_increment primary key,
    fname varchar(50) not null,
    phone varchar(50),
    birthday datetime,
    ftype char(1)
);

commit;
select * from phonebook;